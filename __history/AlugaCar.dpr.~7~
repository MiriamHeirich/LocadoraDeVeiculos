program AlugaCar;

uses
  Vcl.Forms,
  uMenuPrincipal in 'Units\uMenuPrincipal.pas' {Form1},
  uLogin in 'Login\uLogin.pas' {frmLogin},
  uDTMConexao in 'Conexao\uDTMConexao.pas' {DataModule1: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  AApplication.CreateForm(TfrmLogin, frmLogin);
  AApplication.CreateForm(TDataModule1, DataModule1);
  mLogin:=TfrmLogin.Create(nil);
  frmLogin.ShowModal;
  Application.CreateForm(TfrmLogin, frmLogin);
  frmLogin.Hide;
  frmLogin.Free;
  Application.Run;
end.
